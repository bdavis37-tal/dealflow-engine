      "p75_days": 600,
      "note": "Pre-seed to seed: typically 14-20 months"
    },
    "seed_to_series_a": {
      "median_days": 766,
      "p25_days": 480,
      "p75_days": 1100,
      "note": "Seed to A: median 25 months (Carta 2024). Many companies never raise A."
    },
    "series_a_to_b": {
      "median_days": 540,
      "p25_days": 360,
      "p75_days": 730,
      "note": "A to B: median 18 months"
    },
    "series_b_to_c": {
      "median_days": 480,
      "p25_days": 330,
      "p75_days": 660,
      "note": "B to C: median 16 months"
    },
    "series_c_to_exit": {
      "median_days": 900,
      "p25_days": 540,
      "p75_days": 1500,
      "note": "C to exit: highly variable, 18-60 months"
    }
  },

  "stage_transition_probabilities": {
    "_source": "Cambridge Associates, First Round Capital State of Seed 2024, Correlation Ventures",
    "pre_seed_to_seed": 0.45,
