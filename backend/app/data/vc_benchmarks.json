      "median_dilution": 0.12,
      "range": [0.08, 0.18],
      "note": "IPO option pool + underwriter overallotment; assumes no additional dilutive rounds"
    }
  },

  "exit_multiples": {
    "_source": "PitchBook 2024, Carta, Goldman Sachs TMT Precedent Transactions",
    "global": {
      "ma_exit_arr_multiple": {
        "p25": 3.0, "median": 5.5, "p75": 9.0, "p90": 15.0,
        "note": "SaaS M&A: 3-9x ARR typical; premium for high-growth"
      },
      "ipo_arr_multiple": {
        "p25": 8.0, "median": 12.0, "p75": 20.0, "p90": 35.0,
        "note": "IPO valuations 2022-2024; premium for category leaders"
      },
      "secondary_arr_multiple_discount": 0.15,
      "note": "Secondaries typically 10-20% discount to primary round price"
    }
  },

  "burn_multiple_benchmarks": {
    "_source": "David Sacks / Craft Ventures, a16z internal data",
    "definition": "Net Burn / Net New ARR — lower is better",
    "exceptional": { "max": 0.5, "description": "Best-in-class efficiency; Series A fundable" },
    "good": { "min": 0.5, "max": 1.0, "description": "Efficient growth; strong Series A" },
    "fair": { "min": 1.0, "max": 1.5, "description": "Acceptable; watch trajectory" },
    "poor": { "min": 1.5, "max": 2.0, "description": "Inefficient; Series A difficult" },
    "very_poor": { "min": 2.0, "description": "Unsustainable; restructure required" }
  },

  "rule_of_40_benchmarks": {
    "definition": "Revenue Growth % + EBITDA Margin % >= 40",
    "excellent": { "min": 60, "description": "Top-quartile SaaS; premium valuation justified" },
    "strong": { "min": 40, "max": 60, "description": "Rule of 40 satisfied; institutional quality" },
    "fair": { "min": 25, "max": 40, "description": "Below rule of 40; growing into efficiency" },
    "weak": { "max": 25, "description": "Rule of 40 miss; re-examine unit economics" }
  },

