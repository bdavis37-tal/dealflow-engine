      "median_dilution": 0.18,
      "p25_dilution": 0.14,
      "p75_dilution": 0.22,
      "typical_raise_range_usd_m": [15.0, 50.0],
      "note": "Series B: 15-22% typical"
    },
    "series_c": {
      "median_dilution": 0.15,
      "p25_dilution": 0.10,
      "p75_dilution": 0.20,
      "typical_raise_range_usd_m": [30.0, 100.0],
